{
  "version": 2,
  "tags": [
    {
      "name": "mwc-star-rating",
      "description": "5-star rating element",
      "properties": [
        {
          "name": "value",
          "type": "Number",
          "description": "The current rating value (0-5)",
          "default": 0
        },
        {
          "name": "icon",
          "type": "String",
          "description": "The icon to use. Values should correspond to the configured icon font.",
          "default": "star"
        },
      ],
      "events": [
        {
          "name": "rating-selected",
          "summary": "Fires when the icon for a rating value is clicked"
        }
      ],
      "slots": [],
      "cssProperties": [
        {
          "name": "--mwc-star-rating-text-color",
          "description": "Main Text Color",
          "type": "Color"
        },
        {
          "name": "--mwc-star-rating-text-hover-color",
          "description": "Hover Text Color",
          "type": "Color"
        },
        {
          "name": "--mwc-star-rating-text-select-color",
          "description": "Selected Text Color",
          "type": "Color"
        }
      ]
    }
  ]
}
